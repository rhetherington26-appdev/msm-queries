<h1>List of all actors</h1>

<hr>

<table border="1">
 <tr>
  <td>ID</td>
  <td>Image</td>
  <td>Name</td>
  <td>Dob</td>
  <td></td>
 </tr>
 <tr><% @list_of_actors.each do |instance|%>
  <td><%= instance.id%></td>
  <td><img src="http://robohash.org/<%= instance.image%>"></td>
  <td><%= instance.name%></td>
  <td><%= instance.dob%></td>
  <td><a href="/actors/<%= instance.id%>">Show Details</td>
  </tr>
  <% end%>
</table>


 
  


 





 


