<h1>Actor <%= @actor.id%> details</h1>

<a href="/actors">Go Back</a>

<hr>

<dl>
  <dt>
    Name
  </dt>
  <dd>
    <%= @actor.name%>
  </dd>

  <dt>
    Dob
  </dt>
  <dd>
    <%= @actor.dob%>
  </dd>

  <dt>
    Bio
  </dt>
  <dd>
    <%= @actor.bio%>
  </dd>

  <dt>
    Image
  </dt>
  <dd>
    <img src="http://robohash.org/<%= @actor.id%>">
  </dd>

  <dt>
    Created at
  </dt>
  <dd>
    <%= time_ago_in_words(@actor.created_at)%>
  </dd>

  <dt>
    Updated at
  </dt>
  <dd>
    <%= time_ago_in_words(@actor.created_at)%>
  </dd>
</dl>

<hr>
